sub:ag {
	:A dct:hasPart :A1 , :A2 ;
		dct:title "Accessible"@en ;
		a :FAIR-Principle ;
		rdfs:isDefinedBy :thisdoc .
	
	:A1 dct:description "(meta)data are retrievable by their identifier using a standardized communications protocol"@en ;
		dct:hasPart :A1.1 , :A1.2 ;
		dct:title "A1"@en ;
		a :FAIR-SubPrinciple ;
		rdfs:isDefinedBy :thisdoc .
	
	:A1.1 dct:description "the protocol is open, free, and universally implementable"@en ;
		dct:title "A1.1"@en ;
		a :FAIR-SubPrinciple ;
		rdfs:isDefinedBy :thisdoc .
	
	:A1.2 dct:description "the protocol allows for an authentication and authorization procedure, where necessary"@en ;
		dct:title "A1.2"@en ;
		a :FAIR-SubPrinciple ;
		rdfs:isDefinedBy :thisdoc .
	
	:A2 dct:description "metadata are accessible, even when the data are no longer available"@en ;
		dct:title "A2"@en ;
		a :FAIR-SubPrinciple ;
		rdfs:isDefinedBy :thisdoc .
	
	:F dct:hasPart :F1 , :F2 , :F3 , :F4 ;
		dct:title "Findable"@en ;
		a :FAIR-Principle ;
		rdfs:isDefinedBy :thisdoc .
	
	:F1 dct:description "(meta)data are assigned a globally unique and persistent identifier"@en ;
		dct:title "F1"@en ;
		a :FAIR-SubPrinciple ;
		rdfs:isDefinedBy :thisdoc .
	
	:F2 dct:description "data are described with rich metadata (defined by R1 below)"@en ;
		dct:title "F2"@en ;
		a :FAIR-SubPrinciple ;
		rdfs:isDefinedBy :thisdoc .
	
	:F3 dct:description "metadata clearly and explicitly include the identifier of the data it describes"@en ;
		dct:title "F3"@en ;
		a :FAIR-SubPrinciple ;
		rdfs:isDefinedBy :thisdoc .
	
	:F4 dct:description "(meta)data are registered or indexed in a searchable resource"@en ;
		dct:title "F4"@en ;
		a :FAIR-SubPrinciple ;
		rdfs:isDefinedBy :thisdoc .
	
	:FAIR dct:hasPart :A , :F , :I , :R ;
		dct:title "FAIR principles"@en ;
		a owl:Class ;
		rdfs:isDefinedBy thisdoc: ;
		rdfs:seeAlso <https://www.dtls.nl/fair-data/fair-principles-explained/> ;
		rdfs:subClassOf sio:000090 .
	
	:FAIR-Principle dct:description "A FAIR principle describes an essential aspects of FAIR."@en ;
		dct:partOf :FAIR ;
		dct:title "FAIR Principle"@en ;
		a owl:Class ;
		rdfs:isDefinedBy :thisdoc ;
		owl:oneOf sub:_1 .
	
	:FAIR-SubPrinciple dct:description "A FAIR SubPrinciple is a sub-principle of a FAIR principle."@en ;
		dct:partOf :FAIR ;
		dct:title "FAIR SubPrinciple"@en ;
		a owl:Class ;
		rdfs:isDefinedBy :thisdoc ;
		rdfs:subClassOf sio:SIO:000137 ;
		owl:oneOf sub:_15 .
	
	:I dct:hasPart :I1 , :I2 , :I3 ;
		dct:title "Interoperable"@en ;
		a :FAIR-Principle ;
		rdfs:isDefinedBy :thisdoc .
	
	:I1 dct:description "(meta)data use a formal, accessible, shared, and broadly applicable language for knowledge representation"@en ;
		dct:title "I1"@en ;
		a :FAIR-SubPrinciple ;
		rdfs:isDefinedBy :thisdoc .
	
	:I2 dct:description "(meta)data use vocabularies that follow FAIR principles"@en ;
		dct:title "I2"@en ;
		a :FAIR-SubPrinciple ;
		rdfs:isDefinedBy :thisdoc .
	
	:I3 dct:description "(meta)data include qualified references to other (meta)data"@en ;
		dct:title "I3"@en ;
		a :FAIR-SubPrinciple ;
		rdfs:isDefinedBy :thisdoc .
	
	:R dct:hasPart :R1 ;
		dct:title "Reusable"@en ;
		a :FAIR-Principle ;
		rdfs:isDefinedBy :thisdoc .
	
	:R1 dct:description "meta(data) are richly described with a plurality of accurate and relevant attributes"@en ;
		dct:hasPart :R1.1 , :R1.2 , :R1.3 ;
		dct:title "R1"@en ;
		a :FAIR-SubPrinciple ;
		rdfs:isDefinedBy :thisdoc .
	
	:R1.1 dct:description "(meta)data are released with a clear and accessible data usage license"@en ;
		dct:title "R1.1"@en ;
		a :FAIR-SubPrinciple ;
		rdfs:isDefinedBy :thisdoc .
	
	:R1.2 dct:description "(meta)data are associated with detailed provenance"@en ;
		dct:title "R1.2"@en ;
		a :FAIR-SubPrinciple ;
		rdfs:isDefinedBy :thisdoc .
	
	:R1.3 dct:description "(meta)data meet domain-relevant community standards"@en ;
		dct:title "R1.3"@en ;
		a :FAIR-SubPrinciple ;
		rdfs:isDefinedBy :thisdoc .
	
