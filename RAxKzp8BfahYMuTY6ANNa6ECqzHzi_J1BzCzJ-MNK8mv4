sub:head {
	sub:ag a np:Assertion .
	
	sub:ig a np:PublicationInfo .
	
	sub:pg a np:Provenance .
	
	this: np:hasAssertion sub:ag ;
		np:hasProvenance sub:pg ;
		np:hasPublicationInfo sub:ig ;
		a np:Nanopublication .
}

sub:ag {
	sub:A dct:hasPart sub:A1 , sub:A2 ;
		dct:title "Accesible"@en ;
		a sub:FAIR-Aspect .
	
	sub:A1 dct:description "(meta)data are retrievable by their identifier using a standardized communications protocol"@en ;
		dct:hasPart sub:A1.1 , sub:A1.2 ;
		dct:title "A1"@en ;
		a sub:FAIR-Principle .
	
	sub:A1.1 dct:description "the protocol is open, free, and universally implementable"@en ;
		dct:title "A1.1"@en ;
		a sub:FAIR-Principle .
	
	sub:A1.2 dct:description "the protocol allows for an authentication and authorization procedure, where necessary"@en ;
		dct:title "A1.2"@en ;
		a sub:FAIR-Principle .
	
	sub:A2 dct:description "metadata are accessible, even when the data are no longer available"@en ;
		dct:title "A2"@en ;
		a sub:FAIR-Principle .
	
	sub:F dct:hasPart sub:F1 , sub:F2 , sub:F3 , sub:F4 ;
		dct:title "Findable"@en ;
		a sub:FAIR-Aspect .
	
	sub:F1 dct:description "(meta)data are assigned a globally unique and persistent identifier"@en ;
		dct:title "F1"@en ;
		a sub:FAIR-Principle .
	
	sub:F2 dct:description "data are described with rich metadata (defined by R1 below)"@en ;
		dct:title "F2"@en ;
		a sub:FAIR-Principle .
	
	sub:F3 dct:description "metadata clearly and explicitly include the identifier of the data it describes"@en ;
		dct:title "F3"@en ;
		a sub:FAIR-Principle .
	
	sub:F4 dct:description "(meta)data are registered or indexed in a searchable resource"@en ;
		dct:title "F4"@en ;
		a sub:FAIR-Principle .
	
	sub:FAIR-Aspect dct:description "A FAIR aspect is one the four aspects that define the FAIR acronym"@en ;
		dct:title "FAIR Aspect"@en ;
		a owl:Class ;
		<http://www.w3.org/2000/01/rdf-schema#subClassOf> sio:SIO:000137 ;
		owl:oneOf sub:_1 .
	
	sub:FAIR-Principle dct:description "A FAIR principle describes an essential component of a FAIR aspect"@en ;
		dct:title "FAIR Principle"@en ;
		a owl:Class ;
		owl:oneOf sub:_15 .
	
	sub:I dct:hasPart sub:I1 , sub:I2 , sub:I3 ;
		dct:title "Interoperable"@en ;
		a sub:FAIR-Aspect .
	
	sub:I1 dct:description "(meta)data use a formal, accessible, shared, and broadly applicable language for knowledge representation"@en ;
		dct:title "I1"@en ;
		a sub:FAIR-Principle .
	
	sub:I2 dct:description "(meta)data use vocabularies that follow FAIR principles"@en ;
		dct:title "I2"@en ;
		a sub:FAIR-Principle .
	
	sub:I3 dct:description "(meta)data include qualified references to other (meta)data"@en ;
		dct:title "I3"@en ;
		a sub:FAIR-Principle .
	
	sub:R dct:hasPart sub:R1 ;
		dct:title "Reusable"@en ;
		a sub:FAIR-Aspect .
	
	sub:R1 dct:description "meta(data) are richly described with a plurality of accurate and relevant attributes"@en ;
		dct:hasPart sub:R1.1 , sub:R1.2 , sub:R1.3 ;
		dct:title "R1"@en ;
		a sub:FAIR-Principle .
	
	sub:R1.1 dct:description "(meta)data are released with a clear and accessible data usage license"@en ;
		dct:title "R1.1"@en ;
		a sub:FAIR-Principle .
	
	sub:R1.2 dct:description "(meta)data are associated with detailed provenance"@en ;
		dct:title "R1.2"@en ;
		a sub:FAIR-Principle .
	
	sub:R1.3 dct:description "(meta)data meet domain-relevant community standards"@en ;
		dct:title "R1.3"@en ;
		a sub:FAIR-Principle .
	
	sub:_1 rdf:first sub:F ;
