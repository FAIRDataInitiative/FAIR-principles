@prefix : <http://semanticscience.org/np/fair/> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix np: <http://www.nanopub.org/nschema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <https://www.w3.org/2002/07/owl#> .

:nphead {
  :np 
    a np:Nanopublication;
	np:hasAssertion :npag;
	np:hasProvenance :nppg;
	np:hasPublicationInfo :npig .
  :npig a np:PublicationInfo .
  :npag a np:Assertion .
  :nppg a np:Provenance .
}
:npig {
  :np
	dct:title "Nanopublication containing the FAIR principles"@en;
	dct:rights <http://creativecommons.org/licenses/by/4.0/> ;
	dct:creator <http://orcid.org/0000-0003-4727-9435>;
	dct:created "2017-05-01T15:42:00+01:00"^^xsd:dateTime ;
	dct:contributor <https://ruben.verborgh.org/profile/#me>, <https://orcid.org/0000-0002-1267-0234>, <http://csarven.ca/#i> .
}
:npag {
 :fair  
   dct:title "FAIR principles"@en; 
   a owl:Class;
   rdfs:subClassOf sio:000090; 
   dct:hasPart :F, :A, :I, :R .

  :FAIR-Aspect 
    a owl:Class;
	rdfs:subClassOf sio:SIO:000137 ; #concept
    dct:title "FAIR Aspect"@en;
	dct:description "A FAIR aspect is one the four aspects that define the FAIR acronym"@en ;
	owl:oneOf (:F :A :I :R) .
	
  :FAIR-Principle
    a owl:Class;
    dct:title "FAIR Principle"@en;
    dct:description "A FAIR principle describes an essential component of a FAIR aspect"@en;
	owl:oneOf (:F1 :F2 :F3 :F4 :A1 :A1.1 :A1.2 :A2 :I1 :I2 :I3 :R1 :R1.1 :R1.2 :R1.3) .
	
  :F
   dct:title "Findable"@en;
   a :FAIR-Aspect;
   dct:hasPart :F1, :F2, :F3, :F4 .
    
  :F1 
    a :FAIR-Principle;
	dct:title "F1"@en;
	dct:description "(meta)data are assigned a globally unique and persistent identifier"@en.
	
  :F2 
    a :FAIR-Principle;
	dct:title "F2"@en;
	dct:description "data are described with rich metadata (defined by R1 below)"@en.
	
  :F3
	a :FAIR-Principle;
	dct:title "F3"@en;
	dct:description "metadata clearly and explicitly include the identifier of the data it describes"@en.
	
   :F4
	a :FAIR-Principle;
	dct:title "F4"@en;
	dct:description "(meta)data are registered or indexed in a searchable resource"@en.

   :A
    dct:title "Accesible"@en;
	a :FAIR-Aspect;
	dct:hasPart :A1, :A2 .
	
   :A1 
    a :FAIR-Principle;
	dct:title "A1"@en;
	dct:description "(meta)data are retrievable by their identifier using a standardized communications protocol"@en;
	dct:hasPart :A1.1, :A1.2 .	

	:A1.1
    a :FAIR-Principle;
	dct:title "A1.1"@en;
	dct:description "the protocol is open, free, and universally implementable"@en.
	
   :A1.2
    a :FAIR-Principle;
	dct:title "A1.2"@en;
	dct:description "the protocol allows for an authentication and authorization procedure, where necessary"@en.
	
   :A2
    a :FAIR-Principle;
	dct:title "A2"@en;
	dct:description "metadata are accessible, even when the data are no longer available"@en.

	:I
    dct:title "Interoperable"@en;
	a :FAIR-Aspect ;
	dct:hasPart :I1, :I2, :I3 .
	
	:I1
    a :FAIR-Principle;
	dct:title "I1"@en;
	dct:description "(meta)data use a formal, accessible, shared, and broadly applicable language for knowledge representation"@en.
	
	:I2
    a :FAIR-Principle;
	dct:title "I2"@en;
	dct:description "(meta)data use vocabularies that follow FAIR principles"@en.
	
	:I3
    a :FAIR-Principle;
	dct:title "I3"@en;
	dct:description "(meta)data include qualified references to other (meta)data"@en.
	
	:R
    dct:title "Reusable"@en;
	a :FAIR-Aspect ;
	dct:hasPart :R1 .
	
	:R1
    a :FAIR-Principle;
	dct:title "R1"@en;
	dct:description "meta(data) are richly described with a plurality of accurate and relevant attributes"@en;
	dct:hasPart :R1.1, :R1.2, :R1.3 .
	
	:R1.1
    a :FAIR-Principle;
	dct:title "R1.1"@en;
	dct:description "(meta)data are released with a clear and accessible data usage license"@en.
	
	:R1.2
    a :FAIR-Principle;
	dct:title "R1.2"@en;
	dct:description "(meta)data are associated with detailed provenance"@en . 

	:R1.3
    a :FAIR-Principle;
	dct:title "R1.3"@en;
	dct:description "(meta)data meet domain-relevant community standards"@en .	
}

:nppg {
  :npag 
	prov:wasQuotedFrom <https://www.nature.com/articles/sdata201618> .
}
